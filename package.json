{
  "name": "server",
  "version": "1.0.0",
  "description": "A fullstack project built Vue and NodeJS",
  "main": "index.js",
  "engines": {
    "node": "15.2.1",
    "npm": "7.0.11"
  },
  "type": "module",
  "scripts": {
    "test": "mocha --timeout 5000",
    "serve": "nodemon server.js",
    "start": "node server.js",
    "install-frontend": "cd frontend && NODE_ENV=development && npm install && NODE_ENV=production",
    "build-frontend": "cd frontend && npm run build",
    "heroku-postbuild": "npm install && npm run install-frontend && npm run build-frontend"
  },
  "keywords": [
    "RestAPI",
    "Express",
    "MongoDB",
    "Moongose"
  ],
  "author": "Lydia Lind",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irissiri7/Fullstack-Recipe-Manager.git"
  },
  "homepage": "https://github.com/irissiri7/Fullstack-Recipe-Manager#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.2.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.13.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "sinon": "^9.2.3"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
